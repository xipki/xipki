{
	"basedir":"APP_DIR",
	"properties":{
		"profiles.dir":"xipki/ca-setup/certprofile",
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf":"REPLACEME-CA_SIGNERCONF"
	},
	"signers":[{
		"name":"responder1",
		"type":"pkcs12",
		"conf":{
			"value":"password=1234,keystore=file:xipki/keycerts/tlskeys/server/tls-server.p12,algo=SHA256withECDSA"
		}
	}],
	"requestors":[{
		"name":"requestor1",
		"type":"cert",
		"binaryConf":{
			"file":"xipki/keycerts/tlskeys/client/tls-client-cert.der"
		}
	},{
		"name":"requestor-mac1",
		"type":"pbm",
		"conf":{
			"value":"123456"
		}
	}],
	"users":[{
		"name":"user1",
		"active":true,
		"password":"password1"
	}],
	"publishers":[],
	"profiles":[{
		"name":"rootca",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-rootca.json"
		}
	},{
		"name":"scep",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-scep.json"
		}
	},{
		"name":"ocsp",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-ocsp.json"
		}
	},{
		"name":"smime",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-smime.json"
		}
	},{
		"name":"tls",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls.json"
		}
	},{
		"name":"tls-c",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls-c.json"
		}
	}],
	"cas":[{
		"name":"myca1",
		"aliases":[
			"myca"
		],
		"caInfo":{
			"caUris":{
				"crlUris":[
					"http://ca.myorg.org/crl"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp"
				]
			},
			"cmpControl":{
				"send.ca":true,
				"send.certchain":false,
				"messagetime.bias":300,
				"rr.aki.required":true,
				"group.enroll":true,
				"protection.sigalgo":"SHA256withRSA:SHA256withECDSA",
				"popo.sigalgo":"SHA256withRSA:SHA256withECDSA:SHA256withDSA:SM3withSM2:ED25519:ED448:DHPOP-X25519:DHPOP-X448"
			},
			"cmpResponderName":"responder1",
			"crlControl":{
				"update.mode":"interval",
				"fullcrl.intervals":7,
				"overlap.days":3,
				"interval.time":"02:00"
			},
			"duplicateKey":true,
			"duplicateSubject":true,
			"expirationPeriod":365,
			"genSelfIssued":{
				"certOutform":"der",
				"subject":"REPLACEME-SUBJECT",
				"profile":"rootca",
				"serialNumber":"1"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"10y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest","scep"],
			"saveReq":false,
			"scepControl":{
				"cacert.included":true,
				"certchain.included":false,
				"signercert.included":true,
				"support.getcrl":true
			},
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"cutoff"
		},
		"profiles":[
			"ocsp",
			"scep",
			"smime",
			"tls",
			"tls-c"
		],
		"publishers":[],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}],
		"users":[{
			"userName":"user1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

