{
  "metadata": {
    "category": "A",
    "details": "certprofile tls"
  },
  "certLevel": "EndEntity",
  "maxSize": 8400,
  "validity": "5y",
  "notBeforeTime": "current",
  "keypairGeneration": "SECP256R1",
  "keyAlgorithms": ["RSA-2048","RSA-3072","RSA-4096","SECP256R1","SECP384R1",
    "SECP521R1","BRAINPOOLP256R1","BRAINPOOLP384R1","BRAINPOOLP512R1",
    "SM2P256V1","FRP256V1","X25519","X448","MLKEM512","MLKEM768","MLKEM1024",
    "MLKEM768-RSA2048-SHA3-256",
    "MLKEM768-RSA3072-SHA3-256",
    "MLKEM768-RSA4096-SHA3-256",
    "MLKEM768-X25519-SHA3-256",
    "MLKEM768-ECDH-P256-SHA3-256",
    "MLKEM768-ECDH-P384-SHA3-256",
    "MLKEM768-ECDH-BRAINPOOLP256R1-SHA3-256",
    "MLKEM1024-RSA3072-SHA3-256",
    "MLKEM1024-ECDH-P384-SHA3-256",
    "MLKEM1024-ECDH-BRAINPOOLP384R1-SHA3-256",
    "MLKEM1024-X448-SHA3-256",
    "MLKEM1024-ECDH-P521-SHA3-256"
  ],
  "subject": [{
      "type": "country",
      "minOccurs": 0
    },{
      "type": "organization",
      "minOccurs": 0
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName",
      "toSAN": "DNSName",
      "regex": ":FQDN"
    }
  ],
  "extensions": [{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "inRequest": "optional",
      "subjectAltName": {
        "modes": ["IPAddress","DNSName"]
      }
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["keyEncipherment","dataEncipherment"]
          },{
            "appliesTo": ["X25519","X448","MLKEM512","MLKEM768","MLKEM1024",
              "MLKEM768-RSA2048-SHA3-256",
              "MLKEM768-RSA3072-SHA3-256",
              "MLKEM768-RSA4096-SHA3-256",
              "MLKEM768-X25519-SHA3-256",
              "MLKEM768-ECDH-P256-SHA3-256",
              "MLKEM768-ECDH-P384-SHA3-256",
              "MLKEM768-ECDH-BRAINPOOLP256R1-SHA3-256",
              "MLKEM1024-RSA3072-SHA3-256",
              "MLKEM1024-ECDH-P384-SHA3-256",
              "MLKEM1024-ECDH-BRAINPOOLP384R1-SHA3-256",
              "MLKEM1024-X448-SHA3-256",
              "MLKEM1024-ECDH-P521-SHA3-256"
            ],
            "required": ["keyEncipherment","dataEncipherment","keyAgreement"]
          }
        ]
      }
    },{
      "type": "ExtendedKeyUsage",
      "required": true,
      "inRequest": "optional",
      "extendedKeyUsage": {
        "required": ["serverAuth"],
        "optional": ["clientAuth"]
      }
    }
  ]
}