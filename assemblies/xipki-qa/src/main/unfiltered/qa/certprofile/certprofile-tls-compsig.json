{
  "metadata": {
    "details": "certprofile tls with composite MLDSA keys",
    "category": "A"
  },
  "certLevel": "EndEntity",
  "certDomain": "RFC5280",
  "maxSize": 8400,
  "validity": "5y",
  "notBeforeTime": "current",
  "keypairGeneration": "MLDSA44-ECDSA-P256-SHA256",
  "keyAlgorithms": [
    "MLDSA44-RSA2048-PSS-SHA256",
    "MLDSA44-RSA2048-PKCS15-SHA256",
    "MLDSA44-ED25519-SHA512",
    "MLDSA44-ECDSA-P256-SHA256",
    "MLDSA65-RSA3072-PSS-SHA512",
    "MLDSA65-RSA3072-PKCS15-SHA512",
    "MLDSA65-RSA4096-PSS-SHA512",
    "MLDSA65-RSA4096-PKCS15-SHA512",
    "MLDSA65-ECDSA-P256-SHA512",
    "MLDSA65-ECDSA-P384-SHA512",
    "MLDSA65-ECDSA-BRAINPOOLP256R1-SHA512",
    "MLDSA65-ED25519-SHA512",
    "MLDSA87-ECDSA-P384-SHA512",
    "MLDSA87-ECDSA-BRAINPOOLP384R1-SHA512",
    "MLDSA87-ED448-SHAKE256",
    "MLDSA87-RSA3072-PSS-SHA512",
    "MLDSA87-RSA4096-PSS-SHA512",
    "MLDSA87-ECDSA-P521-SHA512"
  ],
  "subject": [{
    "type": "country",
    "minOccurs": 0
  },{
    "type": "organization",
    "minOccurs": 0
  },{
    "type": "organizationalUnit",
    "minOccurs": 0
  },{
    "type": "serialNumber",
    "minOccurs": 0
  },{
    "type": "commonName",
    "regex": ":FQDN",
    "toSAN": "DNSName"
  }],
  "extensions": [{
    "type": "SubjectKeyIdentifier",
    "required": true
  },{
    "type": "CRLDistributionPoints"
  },{
    "type": "FreshestCRL"
  },{
    "type": "SubjectAlternativeName",
    "required": true,
    "inRequest": "optional",
    "subjectAltName": {
      "modes": ["DNSName","IPAddress"]
    }
  },{
    "type": "BasicConstraints",
    "critical": true,
    "required": true
  },{
    "type": "AuthorityInfoAccess",
    "required": true,
    "authorityInfoAccess": {
      "includeCaIssuers": true,
      "includeOcsp": true
    }
  },{
    "type": "AuthorityKeyIdentifier",
    "required": true
  },{
    "type": "KeyUsage",
    "critical": true,
    "required": true,
    "inRequest": "optional",
    "keyUsage": {
      "usages": [{
        "required": ["digitalSignature"]
      }]
    }
  },{
    "type": "ExtendedKeyUsage",
    "required": true,
    "inRequest": "optional",
    "extendedKeyUsage": {
      "required": ["serverAuth"],
      "optional": ["clientAuth"]
    }
  }]
}