{
  "metadata": {
    "category": "A",
    "details": "certprofile tls"
  },
  "certLevel": "EndEntity",
  "maxSize": 8400,
  "validity": "5y",
  "notBeforeTime": "current",
  "keypairGeneration": "MLDSA44-P256",
  "keyAlgorithms": ["MLDSA44-RSA2048","MLDSA44-RSA2048-PKCS15","MLDSA44-ED25519",
    "MLDSA44-P256","MLDSA65-RSA3072","MLDSA65-RSA3072-PKCS15","MLDSA65-RSA4096",
    "MLDSA65-RSA4096-PKCS15","MLDSA65-P256","MLDSA65-P384","MLDSA65-BRAINPOOLP256R1",
    "MLDSA65-ED25519","MLDSA87-P384","MLDSA87-BRAINPOOLP384R1","MLDSA87-ED448",
    "MLDSA87-RSA3072","MLDSA87-RSA4096","MLDSA87-P521"],
  "subject": [{
      "type": "country",
      "minOccurs": 0
    },{
      "type": "organization",
      "minOccurs": 0
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName",
      "toSAN": "DNSName",
      "regex": ":FQDN"
    }
  ],
  "extensions": [{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "inRequest": "optional",
      "subjectAltName": {
        "modes": ["IPAddress","DNSName"]
      }
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["digitalSignature"]
          }
        ]
      }
    },{
      "type": "ExtendedKeyUsage",
      "required": true,
      "inRequest": "optional",
      "extendedKeyUsage": {
        "required": ["serverAuth"],
        "optional": ["clientAuth"]
      }
    }
  ]
}
