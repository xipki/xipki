{
  "metadata": {
    "category": "A",
    "details": "certprofile tls with montmomery 25519 curves"
  },
  "certLevel": "EndEntity",
  "maxSize": 8400,
  "validity": "2y",
  "notBeforeTime": "current",
  "keypairGeneration": "X25519",
  "signatureAlgorithms": ["ED25519","ED448"],
  "keyAlgorithms": ["X25519"],
  "subject": [{
      "type": "country",
      "minOccurs": 0
    },{
      "type": "organization",
      "minOccurs": 0
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName",
      "toSAN": "DNSName",
      "regex": ":FQDN"
    }
  ],
  "extensions": [{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["keyAgreement"]
          }
        ]
      }
    },{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "inRequest": "optional",
      "subjectAltName": {
        "modes": ["IPAddress","DNSName"]
      }
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "ExtendedKeyUsage",
      "required": true,
      "inRequest": "optional",
      "extendedKeyUsage": {
        "required": ["serverAuth","clientAuth"]
      }
    }
  ]
}
