{
  "metadata": {
    "category": "A",
    "details": "certprofile SM2 tls sign"
  },
  "certLevel": "EndEntity",
  "maxSize": 8400,
  "validity": "5y",
  "notBeforeTime": "current",
  "keypairGeneration": "SM2P256V1",
  "keyAlgorithms": ["RSA-2048","RSA-3072","RSA-4096","SECP256R1","SECP384R1",
    "SECP521R1","BRAINPOOLP256R1","BRAINPOOLP384R1","BRAINPOOLP512R1",
    "SM2P256V1","FRP256V1","ED25519","ED448","ML-DSA-44","ML-DSA-65",
    "ML-DSA-87"],
  "subject": [{
      "type": "country",
      "minOccurs": 0
    },{
      "type": "organization",
      "minOccurs": 0
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName",
      "toSAN": "DNSName",
      "regex": ":FQDN"
    }
  ],
  "extensions": [{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "inRequest": "optional",
      "subjectAltName": {
        "modes": ["IPAddress","DNSName"]
      }
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["digitalSignature"]
          }
        ]
      }
    },{
      "type": "ExtendedKeyUsage",
      "required": true,
      "extendedKeyUsage": {
        "required": ["serverAuth"]
      }
    }
  ]
}