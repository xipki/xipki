{
  "metadata": {
    "category": "A",
    "details": "certprofile extended"
  },
  "certLevel": "EndEntity",
  "maxSize": 8400,
  "validity": "5y",
  "notBeforeTime": "current",
  "keypairGeneration": "InheritCA",
  "keyAlgorithms": ["RSA-2048","RSA-3072","RSA-4096","SECP256R1","SECP384R1",
    "SECP521R1","BRAINPOOLP256R1","BRAINPOOLP384R1","BRAINPOOLP512R1",
    "SM2P256V1","FRP256V1","ED25519","ED448","ML-DSA-44","ML-DSA-65",
    "ML-DSA-87"],
  "subject": [{
      "type": "country"
    },{
      "type": "organization"
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName",
      "toSAN": "DNSName",
      "regex": ":FQDN"
    }
  ],
  "extensions": [{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "inRequest": "required",
      "subjectAltName": {
        "modes": ["IPAddress","DNSName"]
      }
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["digitalSignature","keyEncipherment","dataEncipherment"]
          },{
            "appliesTo": ["ED25519","ED448","MLDSA44","MLDSA65","MLDSA87"],
            "required": ["digitalSignature"]
          }
        ]
      }
    },{
      "type": "ExtendedKeyUsage",
      "required": true,
      "inRequest": "optional",
      "extendedKeyUsage": {
        "required": ["serverAuth"],
        "optional": ["clientAuth"]
      }
    },{
      "type": "TLSFeature",
      "required": true,
      "critical": true,
      "tlsFeature": {
        "features": [2,5]
      }
    },{
      "type": "SMIMECapabilities",
      "required": true,
      "smimeCapabilities": {
        "capabilities": [{
            "capabilityId": "1.2.840.113549.3.7"
          },{
            "capabilityId": "1.2.840.113549.3.2",
            "parameter": 128
          },{
            "capabilityId": "1.2.840.113549.3.2",
            "parameter": 64
          }
        ]
      }
    },{
      "type": "1.2.3.4.1",
      "required": true,
      "inRequest": "required"
    }
  ]
}