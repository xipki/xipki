{
  "metadata": {
    "category": "A",
    "details": "certprofile subca-complex (with most extensions)"
  },
  "certLevel": "SubCA",
  "maxSize": 8400,
  "validity": "8y",
  "notBeforeTime": "current",
  "keyAlgorithms": ["RSA-2048","RSA-3072","RSA-4096","SECP256R1","SECP384R1",
    "SECP521R1","BRAINPOOLP256R1","BRAINPOOLP384R1","BRAINPOOLP512R1",
    "SM2P256V1","FRP256V1","ED25519","ED448","ML-DSA-44","ML-DSA-65",
    "ML-DSA-87"],
  "subject": [{
      "type": "country"
    },{
      "type": "organization"
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName"
    }
  ],
  "extensions": [{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true,
      "basicConstraints": {
        "pathLen": 1
      }
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["keyCertSign","cRLSign"]
          }
        ]
      }
    },{
      "type": "CertificatePolicies",
      "required": true,
      "certificatePolicies": {
        "certificatePolicyInformations": [{
            "policyIdentifier": "1.2.3.4.5",
            "policyQualifiers": [{
                "type": "cpsUri",
                "value": "http://myorg.org/ca1-cps"
              }
            ]
          },{
            "policyIdentifier": "2.4.3.2.1"
          }
        ]
      }
    },{
      "type": "PolicyMappings",
      "required": true,
      "critical": true,
      "policyMappings": {
        "mappings": [{
            "issuerDomainPolicy": "1.1.1.1.1",
            "subjectDomainPolicy": "2.1.1.1.1"
          },{
            "issuerDomainPolicy": "1.1.1.1.2",
            "subjectDomainPolicy": "2.1.1.1.2"
          }
        ]
      }
    },{
      "type": "PolicyConstrains",
      "required": true,
      "critical": true,
      "policyConstraints": {
        "requireExplicitPolicy": 2,
        "inhibitPolicyMapping": 2
      }
    },{
      "type": "NameConstraints",
      "required": true,
      "critical": true,
      "inRequest": "required",
      "nameConstraints": {
        "permittedSubtrees": [{
            "directoryName": "OU=bad OU, O=myorg organization, C=DE"
          }
        ]
      }
    },{
      "type": "InhibitAnyPolicy",
      "required": true,
      "critical": true,
      "inhibitAnyPolicy": {
        "skipCerts": 1
      }
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "critical": true,
      "inRequest": "required",
      "subjectAltName": {
        "modes": ["otherName","DNSName","IPAddress","rfc822Name",
          "registeredID","directoryName","ediPartyName","uri"]
      }
    },{
      "type": "SubjectInfoAccess",
      "required": true,
      "inRequest": "required",
      "subjectInfoAccess": {
        "accesses": [{
            "accessMethod": "CARepository",
            "accessLocation": {
              "modes": ["directoryName","uri"]
            }
          }
        ]
      }
    }
  ]
}