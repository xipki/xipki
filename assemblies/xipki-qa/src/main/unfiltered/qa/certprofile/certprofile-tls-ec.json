{
  "metadata": {
    "category": "A",
    "details": "certprofile tls"
  },
  "certLevel": "EndEntity",
  "maxSize": 8400,
  "validity": "5y",
  "notBeforeTime": "current",
  "keypairGeneration": "SECP256R1",
  "keyAlgorithms": ["SECP256R1","SECP384R1","SECP521R1","BRAINPOOLP256R1",
    "BRAINPOOLP384R1","BRAINPOOLP512R1","SM2P256V1","FRP256V1"
  ],
  "subject": [{
      "type": "country",
      "minOccurs": 0
    },{
      "type": "organization",
      "minOccurs": 0
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName",
      "toSAN": "DNSName",
      "regex": ":FQDN"
    }
  ],
  "extensions": [{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "inRequest": "optional",
      "subjectAltName": {
        "modes": ["IPAddress","DNSName"]
      }
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["digitalSignature","keyEncipherment","dataEncipherment"]
          }
        ]
      }
    },{
      "type": "ExtendedKeyUsage",
      "required": true,
      "inRequest": "optional",
      "extendedKeyUsage": {
        "required": ["serverAuth"],
        "optional": ["clientAuth"]
      }
    }
  ]
}