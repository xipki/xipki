{
  "metadata": {
    "category": "A",
    "details": "certprofile subca-complex (with most extensions)"
  },
  "certLevel": "SubCA",
  "maxSize": 8400,
  "validity": "8y",
  "notBeforeTime": "current",
  "keyAlgorithms": ["SECP256R1","SECP384R1","SECP521R1","BRAINPOOLP256R1",
    "BRAINPOOLP384R1","BRAINPOOLP512R1","SM2P256V1","FRP256V1","RSA2048",
    "RSA3072","RSA4096","SM2P256V1","ED25519","ED448","MLDSA44","MLDSA65",
    "MLDSA87","MLDSA44-RSA2048","MLDSA44-RSA2048-PKCS15","MLDSA44-ED25519",
    "MLDSA44-P256","MLDSA65-RSA3072","MLDSA65-RSA3072-PKCS15","MLDSA65-RSA4096",
    "MLDSA65-RSA4096-PKCS15","MLDSA65-P256","MLDSA65-P384","MLDSA65-BRAINPOOLP256R1",
    "MLDSA65-ED25519","MLDSA87-P384","MLDSA87-BRAINPOOLP384R1","MLDSA87-ED448",
    "MLDSA87-RSA3072","MLDSA87-RSA4096","MLDSA87-P521"],
  "subject": [{
      "type": "country"
    },{
      "type": "organization"
    },{
      "type": "organizationalUnit",
      "minOccurs": 0
    },{
      "type": "serialNumber",
      "minOccurs": 0
    },{
      "type": "commonName"
    }
  ],
  "extensions": [{
      "type": "SubjectKeyIdentifier",
      "required": true
    },{
      "type": "CRLDistributionPoints"
    },{
      "type": "FreshestCRL"
    },{
      "type": "BasicConstraints",
      "required": true,
      "critical": true,
      "basicConstraints": {
        "pathLen": 1
      }
    },{
      "type": "AuthorityInfoAccess",
      "required": true,
      "authorityInfoAccess": {
        "includeCaIssuers": true,
        "includeOcsp": true
      }
    },{
      "type": "AuthorityKeyIdentifier",
      "required": true
    },{
      "type": "KeyUsage",
      "required": true,
      "critical": true,
      "inRequest": "optional",
      "keyUsage": {
        "usages": [{
            "required": ["keyCertSign","cRLSign"]
          }
        ]
      }
    },{
      "type": "CertificatePolicies",
      "required": true,
      "certificatePolicies": {
        "certificatePolicyInformations": [{
            "policyIdentifier": "2.4.3.2.1"
          },{
            "policyIdentifier": "1.2.3.4.5",
            "policyQualifiers": [{
                "type": "cpsUri",
                "value": "http://myorg.org/ca1-cps"
              }
            ]
          }
        ]
      }
    },{
      "type": "PolicyMappings",
      "required": true,
      "critical": true,
      "policyMappings": {
        "mappings": [{
            "issuerDomainPolicy": "1.1.1.1.1",
            "subjectDomainPolicy": "2.1.1.1.1"
          },{
            "issuerDomainPolicy": "1.1.1.1.2",
            "subjectDomainPolicy": "2.1.1.1.2"
          }
        ]
      }
    },{
      "type": "PolicyConstrains",
      "required": true,
      "critical": true,
      "policyConstraints": {
        "requireExplicitPolicy": 2,
        "inhibitPolicyMapping": 2
      }
    },{
      "type": "NameConstraints",
      "required": true,
      "critical": true,
      "nameConstraints": {
        "permittedSubtrees": [{
            "directoryName": "O=myorg organization, C=DE"
          }
        ],
        "excludedSubtrees": [{
            "directoryName": "OU=bad OU, O=myorg organization, C=DE"
          }
        ]
      }
    },{
      "type": "InhibitAnyPolicy",
      "required": true,
      "critical": true,
      "inhibitAnyPolicy": {
        "skipCerts": 1
      }
    },{
      "type": "SubjectAlternativeName",
      "required": true,
      "critical": true,
      "inRequest": "required",
      "subjectAltName": {
        "modes": ["IPAddress","registeredID","rfc822Name","DNSName",
          "otherName","directoryName","ediPartyName","uri"]
      }
    },{
      "type": "SubjectInfoAccess",
      "required": true,
      "inRequest": "required",
      "subjectInfoAccess": {
        "accesses": [{
            "accessMethod": "CARepository",
            "accessLocation": {
              "modes": ["directoryName","uri"]
            }
          }
        ]
      }
    },{
      "type": "PrivateKeyUsagePeriod",
      "required": true,
      "privateKeyUsagePeriod": {
        "validity": "10y"
      }
    }
  ]
}
