  "cas":[{
    "name":"rootca1",
    "aliases":[
      "rootca"
    ],
    "caInfo":{
      "caUris":{
        "cacertUris":[
          "http://myorg.org/rootca1.der"
        ],
        "crlUris":[
          "http://localhost:8081/dummy/crl/?type=crl&name=rootca1"
        ],
        "ocspUris":[
          "http://localhost:8080/ocsp/responder1"
        ]
      },
      "crlControl":{
        "interval.hours": 24,
        "fullcrl.intervals":90,
        "overlap":"90d",
        "interval.time":"01:00"
      },
      "keypairGenNames": ["software"],
      "expirationPeriod":365,
      "genSelfIssued":{
        "subject":"REPLACEME-SUBJECT",
        "profile":"rootca",
        "serialNumber":"1"
      },
      "keepExpiredCertDays":-1,
      "maxValidity":"10y",
      "nextCrlNo":2,
      "permissions":["all"],
      "saveCert":true,
      "saveKeypair":false,
      "signerType":"${token.type}",
      "signerConf":{
        "value":"${ca.signerConf}"
      },
      "snSize":20,
      "status":"active",
      "validityMode":"cutoff"
    },
    "profiles":[
      "cross",
      "subca",
      "subca-complex",
      "ocsp"
    ],
    "publishers":[
      "ocsp-publisher"
    ],
    "requestors":[{
      "requestorName":"requestor1",
      "permissions":[
        "all"
      ],
      "profiles":[
        "all"
      ]
    }]
