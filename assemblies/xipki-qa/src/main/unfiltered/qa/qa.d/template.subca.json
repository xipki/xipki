{
    "basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf": "REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"subca1",
		"aliases":[
			"subca"
		],
		"caInfo":{
			"caUris":{
				"cacertUris":[
					"http://myorg.org/subca1.der"
				],
				"crlUris":[
					"http://localhost:8081/crl/?type=crl&name=subca1"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder1"
				]
			},
			"cert":{
				"file":"output/subca1.crt"
			},
			"certchain":[{
				"file":"output/rootca1.crt"
			}],
			"crlControl":{
				"interval.hours": 24,
				"fullcrl.intervals":7,
				"overlap":"3d",
				"interval.time":"02:00"
			},
			"ctlogControl":{
				"enabled":true,
				"sslcontext.name":"ctlog1",
				"servers":"https://localhost/ctlog/"
			},
			"keypairGenNames":["software"],
			"expirationPeriod":365,
			"keepExpiredCertDays":-1,
			"maxValidity":"5y",
			"nextCrlNo":2,
			"permissions":["all"],
			"revokeSuspendedControl":{
				"enabled":true,
				"targetReason":"cessationOfOperation",
				"unchangedSince":"15d"
			},
			"saveCert":true,
			"saveKeypair":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"strict"
		},
		"profiles":[
			"qc",
			"ee-complex",
			"constant-ext",
			"smime:smime-alias",
			"smime-legacy",
			"tls:tls-alias1,tls-alias2",
			"tls-c",
			"fixed-partial-subject",
			"tls-dsa", "tls-ec", "tls-ed25519", "tls-rsa", "tls-sm2"
		],
		"publishers":[
			"ocsp-publisher"
		],
		"requestors":[{
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

