{
    "basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf": "REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"subca1",
		"aliases":[
			"subca"
		],
		"caInfo":{
			"caUris":{
				"cacertUris":[
					"http://example.org/subca1.der"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder1"
				]
			},
			"cmpControl":{
				"send.ca":"true",
				"messagetime.bias":"300",
				"rr.aki.required":"true",
				"group.enroll":"true",
				"protection.sigalgo":"REPLACEME-CMP_SIGALGO",
				"popo.sigalgo":"REPLACEME-SIGALGO:REPLACEME-HASHwithRSA:REPLACEME-HASHwithECDSA:REPLACEME-HASHwithDSA:SM3withSM2"
			},
			"cmpResponderName":"responder1",
			"duplicateKey":true,
			"duplicateSubject":true,
			"expirationPeriod":365,
			"extraControl":{
				"revokeSuspendedCerts.enabled":"true",
				"revokeSuspendedCerts.targetReason":"cessationOfOperation",
				"revokeSuspendedCerts.unchangedSince":"15d"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"5y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest"],
			"saveReq":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":63,
			"status":"active",
			"validityMode":"strict"
		},
		"profiles":[
			"qc",
			"ee-complex",
			"tls",
			"tls-c",
			"tls-inc-sn"
		],
		"publishers":[
			"ocsp-publisher"
		],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

