{
    "basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf": "REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"subca1",
		"aliases":[
			"subca"
		],
		"caInfo":{
			"caUris":{
				"cacertUris":[
					"http://example.org/subca1.der"
				],
				"crlUris":[
					"http://ca-subca.example.org/crl"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder1"
				]
			},
			"certchain":[{
				"file":"output/rootca1.crt"
			}],
			"cmpControl":{
				"send.ca":true,
				"messagetime.bias":300,
				"rr.aki.required":true,
				"group.enroll":true,
				"protection.sigalgo":"SHA256withRSA",
				"popo.sigalgo":"Ed25519:Ed448:DHPOP-X25519-SHA256:DHPOP-X448-SHA512"
			},
			"cmpResponderName":"responder1",
			"duplicateKey":true,
			"duplicateSubject":true,
			"expirationPeriod":365,
			"revokeSuspendedControl":{
				"enabled":true,
				"targetReason":"cessationOfOperation",
				"unchangedSince":"15d"
			},
			"dhpocControl":{
				"value":"type=pkcs12,password=1234,keystore=file:xipki/qa/keys/dhpoc.p12"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"5y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest"],
			"saveReq":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"strict"
		},
		"profiles":[
			"ed25519",
			"ed448",
			"x25519",
			"x448"
		],
		"publishers":[
			"ocsp-publisher"
		],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

