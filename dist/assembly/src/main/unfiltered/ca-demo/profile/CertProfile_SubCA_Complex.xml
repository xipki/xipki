      <parameter name="moduluslength">
        <Min>2048</Min>
        <Max>2048</Max>
      </parameter>
      <parameter name="moduluslength">
        <Min>3072</Min>
        <Max>3072</Max>
      </parameter>
    </algorithm>
    <algorithm>
      <algorithm description="EC">1.2.840.10045.2.1</algorithm>
      <ecParameter>
        <curve>
          <oid description="prime256v1">1.2.840.10045.3.1.7</oid>
          <encodings>
            <encoding>4</encoding>
          </encodings>
        </curve>
        <curve>
          <oid description="brainpoolp256r1">1.3.36.3.3.2.8.1.1.7</oid>
          <encodings>
            <encoding>4</encoding>
          </encodings>
        </curve>
      </ecParameter>
    </algorithm>
  </keyAlgorithms>
